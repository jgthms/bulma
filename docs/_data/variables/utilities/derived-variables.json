    "$background",
    "$border",
    "$border-hover",
    "$text",
    "$text-invert",
    "$text-light",
    "$text-strong",
    "$code",
    "$code-background",
    "$pre",
    "$pre-background",
    "$link",
    "$link-invert",
    "$link-visited",
    "$link-hover",
    "$link-hover-border",
    "$link-focus",
    "$link-focus-border",
    "$link-active",
