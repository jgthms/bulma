{
  "by_name": {
    "$primary": {
      "name": "$primary",
      "value": "$turquoise",
      "type": "variable",
      "computed_type": "color",
      "computed_value": "hsl(171, 100%, 41%)"
    },
    "$info": {
      "name": "$info",
      "value": "$cyan",
      "type": "variable",
      "computed_type": "color",
      "computed_value": "hsl(204, 86%,  53%)"
    },
    "$success": {
      "name": "$success",
      "value": "$green",
      "type": "variable",
      "computed_type": "color",
      "computed_value": "hsl(141, 71%,  48%)"
    },
    "$warning": {
      "name": "$warning",
      "value": "$yellow",
      "type": "variable",
      "computed_type": "color",
      "computed_value": "hsl(48,  100%, 67%)"
    },
    "$danger": {
      "name": "$danger",
      "value": "$red",
      "type": "variable",
      "computed_type": "color",
      "computed_value": "hsl(348, 100%, 61%)"
    },
    "$light": {
      "name": "$light",
      "value": "$white-ter",
      "type": "variable",
