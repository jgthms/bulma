    "$family-primary",
    "$family-code",
    "$size-small",
    "$size-normal",
    "$size-medium",
    "$size-large",
    "$custom-colors",
    "$custom-shades",
    "$colors",
    "$shades",
    "$sizes"
  ],
  "file_path": "utilities/derived-variables.sass"
}